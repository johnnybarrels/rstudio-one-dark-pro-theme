/*Options Dialog Box*/

.rstudio-themes-flat .gwt-DialogBox {
    border: solid 1px rgb(75, 75, 75);
    border-radius: 5px;
    background: #8c3030;
    overflow: hidden !important;
    box-shadow: 0 0px 20px rgba(0, 0, 0, 0.6);
