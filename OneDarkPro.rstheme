  background-color: var(--background-color);
  border-color: var(--border-color) !important;
}

body > div.gwt-DialogBox.gwt-DialogBox-ModalDialog > div > table > tbody > tr.dialogMiddle > td.dialogMiddleCenter > div > table > tbody > tr:nth-child(1) > td > table > tbody > tr:nth-child(3) > td > table > tbody > tr > td > div > div:nth-child(3) > div {
  border-color: var(--border-color) !important;
}
div.gwt-DialogBox.gwt-DialogBox-ModalDialog table > tbody > tr > td {
  border-top: var(--border-color);
  border-bottom: var(--border-color);
  /* color: var(--text-color-selected); */
}

body > div.gwt-DialogBox.gwt-DialogBox-ModalDialog.GCHYANPCMX > div > table > tbody > tr.dialogMiddle > td.dialogMiddleCenter > div > table > tbody > tr:nth-child(1) > td > table > tbody > tr:nth-child(3) > td > table > tbody > tr > td > div > div:nth-child(3) > div > div:nth-child(2) > div > div > table > tbody > tr > td {
  border: none;
}

.GCHYANPCPX {
  border: none !important;
}

th.dataGridHeader {
